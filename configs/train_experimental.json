{
    "char_pretrained_path": "res/data/external/char_embed/glove.840B.300d-char.npy",
    "char_vocab_path": "res/data/external/char_embed/glove.840B.300d-char.vocab",
    "wd_entity_coref_file": "res/data/external/stanford_neural_wd_entity_coref_out/ecb_wd_coref.json",
    "glove_path": "res/pretrained/GloVe/glove.6B.300d.txt",

    "train_path": "out/celento/featurex/training_data",
    "dev_path": "out/celento/featurex/dev_data",

    "seed": 1,
    "random_seed": 2048,
    "gpu_num": 0,
    "save_every_topic": false,

    "epochs": 50,
    "batch_size": 32,
    "log_interval": 32,

    "optimizer": "adam",
    "loss": "bce",
    "lr": 0.0001,
    "momentum": 0,
    "patience": 8,
    "regressor_epochs": 1,
    "weight_decay": 0,

    "load_predicted_topics": false,
    "remove_singletons": false,
    "train_init_wd_entity_with_gold": true,
    "test_use_gold_mentions": true,
    "entity_merge_threshold": 0.5,
    "event_merge_threshold": 0.5,
    "dev_th_range": [0.5, 0.6],
    "merge_iters": 2,

    "use_contextembed": false,
    "use_args_feats": true,
    "use_binary_feats": false,
    "use_diff": false,
    "use_mult": true,
    "use_pretrained_char": true,
    "use_sentembed": true,

    "feature_size": 50,
    "char_rep_size": 50,
    "contextembed_size": 1024,
    "sentembed_size": 700
}
